{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "kc12-adf"
		},
		"LS_GEN2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_GEN2'"
		},
		"SQL_DB_LS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SQL_DB_LS'"
		},
		"Github_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"LS_GEN2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://kc123.dfs.core.windows.net/"
		},
		"SQL_DB_LS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "kc123.database.windows.net"
		},
		"SQL_DB_LS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "kc12"
		},
		"SQL_DB_LS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminkc12"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "kc12-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_GEN2_accountKey": {
						"value": "[parameters('LS_GEN2_accountKey')]"
					},
					"SQL_DB_LS_password": {
						"value": "[parameters('SQL_DB_LS_password')]"
					},
					"Github_LS_properties_typeProperties_url": {
						"value": "[parameters('Github_LS_properties_typeProperties_url')]"
					},
					"LS_GEN2_properties_typeProperties_url": {
						"value": "[parameters('LS_GEN2_properties_typeProperties_url')]"
					},
					"SQL_DB_LS_properties_typeProperties_server": {
						"value": "[parameters('SQL_DB_LS_properties_typeProperties_server')]"
					},
					"SQL_DB_LS_properties_typeProperties_database": {
						"value": "[parameters('SQL_DB_LS_properties_typeProperties_database')]"
					},
					"SQL_DB_LS_properties_typeProperties_userName": {
						"value": "[parameters('SQL_DB_LS_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}